<?xml version='1.0' encoding='utf-8'?>
<plugin id="com-dtech-plugins-ios-vibrate" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Vibrate</name>
    <js-module name="Vibrate" src="www/vibrate.js">
        <clobbers target="Vibrate" />
    </js-module>

    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="Vibrate">
                <param name="ios-package" value="Vibrate" />
            </feature>
        </config-file>
        <source-file src="src/ios/Vibrate.swift" />
    </platform>

    <dependency id="cordova-plugin-add-swift-support" version="2.0.2"/>
</plugin>
